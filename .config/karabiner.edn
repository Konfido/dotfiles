        #_{:des "Emacs key bindings [control+keys] (rev 10)"
         :rules [[:!T#Sy :!Cv :!Emacs]
                 :!emacs-mode-disable-app
                 [:!T#Od :delete_forward]
                 [:!T#Oh :delete_or_backspace]
                 [:!T#SOw :!Odelete_or_backspace]
                 [:!T#Oi :tab]
                 [:!T#Oopen_bracket :escape]
                 [:!T#OSm :return_or_enter]
                 [:!T#OSb :left_arrow]
                 [:!T#OSf :right_arrow]
                 [:!T#OSb :left_arrow]
                 [:!T#OSn :down_arrow]
                 [:!T#OSp :up_arrow]
                 [:!T#Sv :page_down]
                 [:!T#Sa :!Cleft_arrow]
                 [:!T#Se :!Cright_arrow]]}
        #_{:des "capital mode/;->right_shift [semicolon as trigger key]"
         :rules [:capital-mode
                 [:q :!Sq]
                 [:w :!Sw]
                 [:e :!Se]
                 [:r :!Sr]
                 [:t :!St]
                 [:y :!Sy]
                 [:u :!Su]
                 [:i :!Si]
                 [:o :!So]
                 [:p :!Sp]
                 [:a :!Sa]
                 [:s :!Ss]
                 [:d :!Sd]
