                 [:delete_or_backspace [:!Tx :!T9 :!Sk]]
                 [:open_bracket [:!Tx :!T9 :open_bracket]]
                 [:close_bracket [:!Tx :!T9 :close_bracket]]]} 
        #_{:des "Emacs switch buffer"
         :rules [:Emacs
                 [:tab :!Ttab :period-mode]
